/* Review Summary */ 
.reviewProgressMainDiv{
    display: flex;
    justify-content: end;
    position: relative;
    border-top: 1px solid #DFDFDF;
    border-bottom: 1px solid #DFDFDF;
}
#reviewProgress {
    counter-reset: progressCounter;
    overflow: hidden;
    padding: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50% , -50%);
    width: 50%;
}
#reviewProgress li {
    counter-increment: progressCounter;
    text-align: center;
    list-style-type: none;
    color:#c0c0c0;
    font-size: 11px;
    float: left;
    position: relative;
    font-weight: 700;
    width: 25%;
}
#reviewProgress li:before {
    content: counter(progressCounter);
    width: 35px;
    height: 35px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 14px;
    font-weight: var(--semi-bold-fw);
    color: #c0c0c0;
    border-radius: 50%;
    margin: 0 auto 6px auto;
    background-color: #f2f2f2;
}
#reviewProgress li.active, #reviewProgress li.done {
    color: var(--green-clr);
}
#reviewProgress li.active:before {
    content: url('../images/icons/check-white.svg');
    border: 3px solid transparent;
}
#reviewProgress li.done:before {
    content: url('../images/icons/check-white.svg');
    outline: 3px solid #fff;
    background-color: var(--green-clr);
    outline-offset: -6px;
}
#reviewProgress li:after {
    content: '';
    width: 100%;
    height: 2px;
    background:  #f2f2f2;
    position: absolute;
    left: -50%;
    top: 16px;
    z-index: -1;
}
#reviewProgress li:first-child:after {
    display: none;
}
#reviewProgress li.done:after, #reviewProgress li.active:before, #reviewProgress li.active:after {
    background:  var(--green-clr);
}
