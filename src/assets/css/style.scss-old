/* Different Text Styles */
.font-style-italic{
    font-style: italic;
}
.text-underline{
    text-decoration: underline;
}

/* Top Navbar */
.navbar-brand{
    flex-shrink: 0;
}
.top-navbar{
    padding: .625rem 2.5rem;    
    background-color: #fff;
    border-bottom: 1px solid #d3d3d3;
}
.top-user-dropdown{
    .nav-link{    
        padding: 0;
    }
    span{
        width: 40px;
        height: 40px;
        border-radius: 50%;
        background-color: #ececec;
        display: flex;
        justify-content: center;
        align-items: center;

    }
    .dropdown-menu{
        background-color: #FFF;
        box-shadow: 0px 0.25px 8px rgba(0, 0, 0, 0.2);
        border-radius: .5rem;
        border: 0;
        transition: all .1s ease-in;
        
        .dropdown-item{
            padding: .575rem 1rem;
            font-size: 15px;

            &:hover{
                background-color: #f2f2f2;
            }
        }
    }
}
/* End of Top Navbar */

/* Choose Client */
.main-section{
    --top-margin: 62px;
    --sidebar-width: 325px;
    margin-top: var(--top-margin);
    min-height: calc(100vh - var(--top-margin));
}
.inner-section{
    height: 100%;
    width: 100%;
    min-height: calc(100vh - var(--top-margin));
    display: flex;
}
.gap-20{
    gap: 1.25rem;
}
.flex-1{
    flex: 1;
}
.sidebar{
    background-color: var(--green-clr);
    min-width: var(--sidebar-width);
    max-width: var(--sidebar-width);
    height: 100%;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    z-index: 100;
    padding-top: var(--top-margin);
    box-shadow: inset -1px 0 0 rgb(0 0 0 / 10%);
    padding-inline:1.25rem;
    transition: all 0.125s ease-in;

    .sidebar-inner{
        padding-top: 3.85rem;
    }
}

.sidebar-nav{
    .nav-item{
        margin-bottom: 15px;
    }
    .nav-item:last-of-type{
        margin-bottom: 0;
    }
    .nav-link{
        border-radius: .5rem;
        background-color: #147D64;
        padding: 1.25rem 1rem;
        display: flex;
        flex-direction: column;
        // align-items: center;
        justify-content: center;
        font-weight: var(--medium-fw);
        
        color: #fff;
        font-size: 15px;
        border: 1px solid transparent;

        &:hover{
            background-color: var(--green-clr);
            border: 1px solid rgba(255,255,255,0.25);
        }
    
        &.active{
            background-color: #fff;
            font-weight: var(--semi-bold-fw);
            color: #000;

            &:hover{
                background-color: rgba(255,255,255,0.95);
            }

            svg path{
                fill: #000;
            }
        }

        svg{
            margin-right: .875rem;
            position: relative;
            top: -1px;

            path{
                fill: #3CB79A;
            }
            circle{
                stroke: #3CB79A;
            }
            rect{
                fill: #3CB79A;
            }
        }
    }
}

.right-area{
    flex: 1;
    height: calc(100vh - var(--top-margin));
    margin-left: var(--sidebar-width);
}
.right-area-wrapper{
    padding-top: 4.5rem;
}
.right-area-inner{
    height: 100%;
    overflow-y: auto;
    padding-bottom: 1.5rem;
    // margin-right: .25rem;
}
.right-area-inner::-webkit-scrollbar{
    display: none;
}
.section-heading-div{
    padding-inline: 3rem;
    padding-top: 1.25rem;
    padding-bottom: .625rem;
    position: sticky;
    top: 0;
    background-color: #fff;
    z-index: 1;

    &.bottom-border{
        border-bottom: 1px solid #E4E4E4;
    }

}
.main-tab-heading{
    position: relative;

    &::before{
        content: '';
        position: absolute;
        width: 29px;
        height: 4px;
        left: 2px;
        top: 100%;
        background-color: var(--green-clr);

    }
}
.search-bar-div{
    position: relative;

    input.form-control{
        background-color: #F5F5F5;
        border: 1px solid #FFF;
        border-radius: 6.5rem;
        min-height: 45px;
        padding-left: 2.5rem;
        transition: all .1s ease-in;

        &::placeholder{
            color: #a5a5a5;
        }

        &:hover{
            background-color: #f4f4f4;
        }

        &:focus{
            outline: 1px solid #dfdfdf;
            background-color: #f4f4f4;
        }
    }

    .search-icon-span{
        position: absolute;
        left: 1.25rem;
        top: 50%;
        transform: translateY(-50%);
    }
}
.new-btn{
    background-color: #0E6651;
    border-radius: 6.5rem;
    padding: .65rem 1.45rem;
    color: #fff;
    font-weight: var(--semi-bold-fw);

    span{
        line-height: 1;

        &.plus{
            font-weight: bold;
            font-size: 1.5rem;
            position: relative;
            top: 1px;
        }
    }
    
    &:hover{
        background-color: var(--green-clr-2);
        color: #fff;
    }
}
.dropdown-item:focus{
    background: none;
    color: currentColor;
}
.new-btn-dropdown{
    .dropdown-menu{
        top: calc(-100% - 3px) !important;
        background-color: #FFF;
        box-shadow: 0px 0.25px 8px rgba(0, 0, 0, 0.2);
        border-radius: .5rem;
        min-width: 255px;
        border: 0;
        transition: all .1s ease-in;

        .dropdown-item{
            padding: .575rem 1rem;
            font-size: 15px;

            &:hover{
                background-color: #f2f2f2;
            }
        }
    }
}
.sort-dropdown-div{
    .dropdown-toggle{
        text-align: left;
        min-width: 200px;
        background-color: #FFF;
        border: 1px solid #E4E4E4;
        border-radius: .5rem;
        padding: .575rem 1rem;
        font-weight: var(--semi-bold-fw);

        &::after{
            content: url('../images/icons/chevron-down.svg');
            border: 0;
            position: absolute;
            right: .75rem;
            bottom: .35rem;
        }
    }

    .dropdown-menu{
        width: 100%;
        top: calc(-100% - 2px) !important;
        background: #FFF;
        border: 1px solid #E4E4E4;
        border-radius: .5rem;
        transition: all .1s ease-in;
        padding: .75rem 0 .25rem;
        
        .dropdown-item{
            padding: .5rem .875rem;
            font-size: 15px;
            margin-bottom: 2px;

            &:hover{
                background: #f2f2f2;
            }

            &.active{
                color: currentColor;
                background: #F3FFFC;
                border: 1px solid #0E6651;
                border-radius: 3px;
                width: calc(100% - 6px);
                margin-left: 3px;

                &::after{
                    content: url('../images/icons/check-green.svg');
                    position: absolute;
                    right: .85rem;
                }
            }
        }
    }
}

.client-list-div{
   padding-top: 1rem;
   padding-inline: 3rem;

   &.less-gap{
    padding-inline: 2.375rem;
   }
}
.client-list-ul{
    .nav-item{
        background: #FFF;
        border: 1px solid #E4E4E4;
        border-radius: .5rem;
        padding: 0.5rem .75rem;
        margin-bottom: .625rem;
        cursor: pointer;

        &:last-child{
            margin-bottom: 0;
        }
        .client-with-actions{
            display: flex;
            justify-content: space-between;
        }

        &.hovered, &:hover{
            .right-action-btns{
                opacity: 1;
                visibility: visible;
                transform: translateY(0)
            }
        }
    }
    .client-name{
        display: flex;
        align-items: center;

        .name-initial-circle{
            width: 47px;
            height: 47px;
            border-radius: 50%;
            background-color: #F8F8F8;
            display: flex;
            justify-content: center;
            align-items: center;
            margin-right: .8rem;
        }
        .nav-link{
            color: #000;
            text-decoration: none;
        }

        &:hover{
            .name-initial-circle{
                background-color: #f5f5f5;
            }
            .nav-link{
                color: var(--green-clr);

                img{
                    transform: translateX(-3px);
                    transition: .1s ease-in all;
                }
            }
        }
    }
}
.list-wrapper{
    margin-bottom: 1.875rem;
    background: #FFF;
    border: 1.5px solid #E6E6E6;
    border-radius: 15px;
    padding: 1.25rem;

    .client-list-ul{
        .nav-item{
            border: 0;
            padding: 0;

            .list-groups{
                margin-top: 0;
            }
        }
    }
}
.right-action-btns{
    display: flex;
    align-items: center;
    gap: 1rem;
    opacity: 0;
    visibility: hidden;
    transition: all 0.125s ease-in;
    transform: translateY(.5rem)
}
.right-action-btn-div{
    .right-action-btn{
        background-color: #f5f5f5;
        border: 1.5px solid #DFDFDF;
        border-radius: 6.5rem;
        font-weight: var(--semi-bold-fw);
        padding: 0.5rem 3.5rem 0.5rem 1rem;
        min-width: 10rem;

        &.collapsed{
            background: #FFF;

            img{
                transform: rotate(0deg);
            }
            
        }

        &:hover{
            background-color: #f5f5f5;
        }

        img{
            position: relative;
            top: 3px;
            transform: rotate(180deg);
        }
    }
    .plus-sign{
        width: 49px;
        height: 49px;
        flex-shrink: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        border-radius: 50%;
        background-color: var(--green-clr);
        position: absolute;
        right: -4px;
        top: 50%;
        transform: translateY(-50%);
        color: #fff;
        font-size: 1.5rem;
        font-weight: var(--semi-bold-fw);
        transition: all 0.125s ease-in;

        &:hover{
            background-color: var(--green-clr-2);
        }
    }
}
.three-dots-dropdown{
    flex-shrink: 0;
}
.three-dots-btn{
    &:hover{
        img{
            filter: brightness(0.5);
        }
    }
}
.common-dropdown-menu{
    top: calc(-100% - 5px) !important;
    background-color: #FFF;
    box-shadow: 0px 0.25px 8px rgba(0, 0, 0, 0.2);
    border-radius: .5rem;
    min-width: 255px;
    border: 0;
    transition: all .1s ease-in;

    .dropdown-item{
        padding: .575rem 1rem;
        font-size: 15px;

        &:hover{
            background-color: #f2f2f2;
        }
    }
}

.list-groups{
    margin-top: 1.275rem;
}
.lsit-div{
    display: flex;
    flex-direction: column;
}
.list-item{
    background-color: #FFF;
    border: 1.5px solid #E6E6E6;
    border-radius: .5rem;
    margin-bottom: .625rem;

    .list-item{
        border: 0;
        background-color: #f9f9f9;
    }

    &-inner{
        display: flex;
        justify-content: space-between;
        padding: .625rem .875rem;
    }
    
    .list-item-detail p{
        position: relative;

        &::before{
            content: '';
            position: absolute;
            width: 14px;
            height: 2px;
            left: 2px;
            top: 100%;
            background-color: var(--green-clr);
    
        }
    }

    .list-item-detail label{
        font-size: 13px;
    }
}
.inner-report-div{
    padding: .625rem .875rem;    
}
.report-scenario-span{
    background-color: #F5F5F5;
    border-radius: 6.5rem;
    padding: .35rem 0.85rem;
    display: inline-block;
    color: #6b6b6b;
    font-weight: var(--semi-bold-fw);
    font-size: 13px;
}
.list-item-actions{
    display: flex;
    align-items: center;

    .date-label{
        margin-right: 1.125rem;
        display: flex;
        align-items: center;

        img{
            margin-right: .225rem;
        }
    }
    .scenario-add-report-btn{
        background-color: var(--green-clr);
        color: #fff;
        font-size: 13px;
        border-radius: 6.5rem;
        font-weight: var(--medium-fw);
        padding: 0.45rem .9rem;

        &:hover{
            background-color: var(--green-clr-2);
        }
    }
}
.round-btns{
    display: flex;
    align-items: center;
    gap: 0.625rem;
    margin-right: 1rem;
}
.round-btn{
    background-color: #FFF;
    border: 1px solid #DFDFDF;
    width: 33px;
    height: 33px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    outline: 0;
    box-shadow: 0;

    span{
        display: none;
        font-size: .875rem;
        font-weight: var(--semi-bold-fw);
        color: #fff;
    }

    svg{
        flex-shrink: 0;
    }

    &:hover{
        width: auto;
        background-color: var(--green-clr);
        border-radius: 6.5rem;
        border: 0;

        span{
            display: inline;
        }
        svg {
            margin-left: .45rem;

            path{
                fill: #fff;
            }

            rect{
                stroke: #fff;
            }
        }
    }
}
.report-round-btn{
    width: auto;
    background-color: #F5F5F5;
    border-radius: 6.5rem;
    border: 1px solid #DFDFDF;
    
    span{
        display: inline;
        color: #000;
    }

    svg{
        margin-left: .45rem;
    }

    .chevron-up{
        position: relative;
        top: 3px;
    }

    &:hover{
        background-color: #f3f3f3;
        border: 1px solid #DFDFDF;

        svg {

            path{
                fill: currentColor;
            }

            rect{
                stroke: currentColor;
            }
        }
    }

    &.collapsed{
        background-color: #FFF;
        border: 1px solid #DFDFDF;
        width: 33px;
        height: 33px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        outline: 0;
        box-shadow: 0;

        svg{
            margin-left: 0;
        }

        span{
            display: none;
            font-size: .875rem;
            font-weight: var(--semi-bold-fw);
            color: #fff;
        }

        .chevron-up{
            display: none;
        }

        &:hover{
            width: auto;
            background-color: var(--green-clr);
            border-radius: 6.5rem;
            border: 0;
    
            span{
                display: inline;
                color: #fff;
            }
            svg {
                margin-left: .45rem;
                path{
                    fill: #fff;
                }
    
                rect{
                    stroke: #fff;
                }
            }
        }
    }
}
.view-btn{
    font-weight: var(--semi-bold-fw);
    font-size: .875rem;
    text-transform: uppercase;
    color: #9d9d9d;

    &:hover{
        color: #000;
    }

}
.common-modal{
    .modal-dialog{
        margin: 0 auto;
        height: 100%;
        max-width: 540px;
        display: flex;
        align-items: center;
        width: 100%;
        justify-content: center;
    }
    .modal-content{
        border-radius: .75rem;
    }
    .modal-header{
        border-bottom: 0;
    }
    .btn-close{
        background: none;
        opacity: 1;
        display: flex;
        align-items: center;
        justify-content: center;

        &:hover, &:focus{
            box-shadow: none;
            outline: 1px solid #f0f0f0;
        }
    }
    .gap-13{
        gap: 13px;
    }

    .yes-delete-btn{
        min-width: 225px;
        background-color: var(--green-clr);
        padding: .75rem 2rem;
        color: #fff;
        font-weight: var(--semi-bold-fw);
        font-size: .875rem;
        border-radius: 6.5rem;

        &:hover{
            background-color: var(--green-clr-2);
        }
    }

    .modal-cancel-btn{
        min-width: 225px;
        background: #FFF;
        border: 1px solid #DFDFDF;
        border-radius: 6.5rem;
        font-weight: var(--semi-bold-fw);
        font-size: .875rem;
        padding: .75rem 2rem;

        &:hover{
            background-color: #f4f4f4;
        }
    }

}
/* End of Choose Client */

/* Individual Client */
.side-client-label{
    color: #8f8f8f;
    display: flex;
    align-items: center;
    margin-left: 3rem;
    margin-top: .25rem;

    .btn{
        flex-shrink: 0;
        width: 19px;
        height: 19px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        background-color: #ececec;
        padding: 0;
        margin-left: 0.25rem;
        position: relative;
        top: 2px;

        img{
            flex-shrink: 0;
        }
    }
}
.main-cleint-name-div{
    padding-inline: 1.25rem;
    padding-top: 1.25rem;
    padding-bottom: .625rem;
    position: sticky;
    top: 0;
    background-color: #fff;
    z-index: 1;
    border-bottom: 1px solid #E4E4E4;
}
.main-client-heading{
    display: flex;
