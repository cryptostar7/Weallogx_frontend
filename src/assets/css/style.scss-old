:root{
    --green-clr: #0E6651;
    --green-clr-2: #147d64;
    --grey-clr: #9d9d9d;
    --grey-clr-2: #b1b1b1;
    --grey-clr-3: #b0b0b0;
    --thin-fw: 100;
    --extra-light-fw: 200;
    --light-fw: 300;
    --regular-fw: 400;
    --medium-fw: 500;
    --semi-bold-fw: 600;
    --bold-fw: 700;
    --extra-bold-fw: 800;
    --black-fw: 900;
}

body{
    font-family: 'Inter', sans-serif;
    color: #000;
}
body::-webkit-scrollbar{
    display: none;
}
::-webkit-scrollbar {
    width: .5rem;
  }
  
::-webkit-scrollbar-track {
    box-shadow: none; 
    border-radius: .5rem;
}
   
::-webkit-scrollbar-thumb {
    background: #ababab; 
    border-radius: .5rem;
}
  
  /* Handle on hover */
::-webkit-scrollbar-thumb:hover {
    background: #555; 
}
.not-scollable{
    overflow: hidden;
}
.green-clr{
    color: var(--green-clr);
}
.grey-clr{
    color: var(--grey-clr);
}
.grey-clr-2{
    color: var(--grey-clr-2);
}
.grey-clr-3{
    color: var(--grey-clr-3);
}
.green-bg{
    background-color: var(--green-clr);
}
.p-relative{
    position: relative;
}
.form-control:focus, .btn:focus, .btn:active, .btn:active:focus, .form-select:focus{
    box-shadow: none;
}
.no-before{
    &::before{
        display: none;
    }
}
.cursor-pointer{
    cursor: pointer;
}
.no-after{
    &::after{
        display: none;
    }
}
