    border-radius: .75rem;
    margin-top: 1.75rem;
    overflow: hidden;

    .comparative-vehicle-tabs{
        .nav-item{
            flex: 1;
            
            .nav-link{
                width: 100%;
                padding: .875rem .5rem;
                border: 0;
                color: #aeaeae;

                .form-check-input{
                    background-color: #dfdfdf;
                }
                .form-check-input{
                    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='-4 -4 8 8'%3e%3ccircle r='3' fill='%23fff'/%3e%3c/svg%3e");
                    background-position: left center;
                } 

                &.active{
                    background-color: var(--green-clr);
                    border-radius: .75rem .75rem 0 0;
                    color: #fff;

                    .form-check-input{
                        background-color: #fff;
                    }
                    .form-check-input{
                        background-image: url("data:image/svg+xml,%3Csvg width='12' height='12' viewBox='0 0 15 15' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Ccircle cx='7.5' cy='7.5' r='7.5' fill='%230E6651'/%3E%3C/svg%3E");
                        background-size: 20px;
                        background-position: right 1.25px center;
                    }   
                }

                &:hover{
                    border: 0;                }
            }

        }
        .custom-switch{
            display: flex;
            align-items: center;
            justify-content: center;

            input{
                margin-right: .25rem;
            }
        }
    }

    .tab-content{
        padding: 1.25rem 1.875rem 2rem;
    }
}
/* End of Create New Scenario - Comparative Vehicle*/ 




/* Media Query Starts */
@media screen and (max-width: 1199.99px){
    .right-area{
        margin-left: 0;
    }
    .top-navbar{
        .container-fluid{
            flex-wrap: nowrap;
        }

        .navbar-collapse{
            display: flex;
        }
    }
    .sidebar{
        left: calc(-1 * var(--sidebar-width));

        &.show{
            left: 0;
            
            &::before{
                content: '';
                position: fixed;
                inset: 0;
                left: var(--sidebar-width);
                background-color: rgba(0, 0, 0, 0.25);
            }

        }
    }
}
@media screen and (max-width: 1200px){
    .illusnTableMainDiv .table thead {
      display: none;
    }
    .illusnTableMainDiv .table,
    .illusnTableMainDiv .table tbody,
    .illusnTableMainDiv .table tr,
    .illusnTableMainDiv .table td {
      display: block;
      width: 100%;
    }
    .illusnTableMainDiv .table tr {
      margin-bottom: 15px;
    }
    .illusnTableMainDiv .table td {
      padding-left: 50%;
      text-align: left;
      position: relative;
    }
    .illusnTableMainDiv .table td::before {
      content: attr(data-label);
      position: absolute;
      left: 0;
      width: 50%;
      padding-left: 15px;
      font-size: 15px;
      font-weight: bold;
      text-align: left;
    }
}
@media screen and (min-width: 768px) and (max-width: 1199.99px){
    .main-tab-heading{
        flex-basis: 45%;
    }
    .search-bar-div{
        flex-basis: 50%;
    }
}
